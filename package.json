{
  "name": "leanne1-Express-REST",
  "version": "0.0.0",
  "description": "Express REST service",
  "main": "index.js",
  "env": {
    "dev": "development"
  },
  "scripts": {
    "mocha": "cross-env BABEL_ENV=test mocha --require babel-register config/mocha/setup.test.js --opts config/mocha/mocha.server.opts src/**/*.test.js",
    "dev": "export APP_ENV=$npm_package_env_dev DEBUG=app:* && cross-env NODE_ENV=$npm_package_env_dev nodemon src/index --exec babel-node",
    "build": "babel src -d build",
    "start": "node build/index",
    "test": "npm run mocha",
    "tdd": "npm run mocha -- -watch",
    "test:coverage": "nyc npm run mocha && nyc report --reporter=lcov",
    "lint": "eslint .",
    "nsp": "nsp check"
  },
  "pre-push": [
    "lint",
    "nsp",
    "test"
  ],
  "keywords": [],
  "author": "leanne1",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "config": "^1.30.0",
    "express": "^4.16.3",
    "fawn": "^2.1.5",
    "helmet": "^3.12.0",
    "joi": "^13.2.0",
    "joi-objectid": "^2.0.0",
    "joi-password-complexity": "^2.0.1",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mongoose": "^5.0.18",
    "morgan": "^1.9.0",
    "winston": "^2.4.2",
    "winston-mongodb": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "debug": "^3.1.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nsp": "^3.2.1",
    "nyc": "^11.8.0",
    "pre-push": "^0.1.1",
    "sinon": "^5.0.7"
  }
}
